{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "es2015",
    "target": "es5",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "lib": [
      "es2015",
      "dom"
    ],
  },
  "include": [
    "./lib/*.ts"
  ]
}